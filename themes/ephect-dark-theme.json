      }
    },
    {
      "name": "Ephect Liquid Expressions",
      "scope": [
        "meta.liquid.double-braces.phpx",
        "meta.liquid.single-braces.phpx"
      ],
      "settings": {
        "foreground": "#D4D4D4"
      }
    },
    {
      "name": "Ephect Liquid Punctuation",
      "scope": [
        "punctuation.definition.liquid.begin.phpx",
        "punctuation.definition.liquid.end.phpx"
      ],
      "settings": {
        "foreground": "#569CD6",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Ephect Liquid Variables",
      "scope": [
        "variable.other.liquid.phpx"
      ],
      "settings": {
        "foreground": "#9CDCFE"
      }
    },
    {
      "name": "Ephect Liquid Operators",
      "scope": [
        "keyword.operator.liquid.phpx"
      ],
      "settings": {
        "foreground": "#C586C0"
      }
    },
    {
      "name": "Ephect Liquid Numbers",
      "scope": [
        "constant.numeric.liquid.phpx"
      ],
      "settings": {
        "foreground": "#B5CEA8"
      }
    },
    {
      "name": "Ephect Liquid Strings",
      "scope": [
        "string.quoted.single.liquid.phpx",
        "string.quoted.double.liquid.phpx"
      ],
      "settings": {
        "foreground": "#CE9178"
      }
