{
  "$schema": "vscode://schemas/color-theme",
  "name": "Ephect Midnight",
  "type": "dark",
  "colors": {
    "editor.background": "#0f111a",
    "editor.foreground": "#c8d1e0"
  },
  "tokenColors": [
    {
      "scope": ["keyword.control.ephect", "punctuation.definition.keyword.ephect"],
      "settings": {
        "foreground": "#61afef",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["variable.other.ephect", "variable.other.property.ephect"],
      "settings": {
        "foreground": "#e5c07b"
      }
    },
    {
      "scope": [
        "meta.interpolation.ephect",
        "punctuation.definition.template-expression.begin.ephect",
        "punctuation.definition.template-expression.end.ephect"
      ],
      "settings": {
        "foreground": "#98c379"
      }
    },
    {
      "scope": ["meta.embedded.block.html.ephect"],
      "settings": {
        "foreground": "#abb2bf"
      }
    }
  ]
}
