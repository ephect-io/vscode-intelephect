{
  "$schema": "vscode://schemas/color-theme",
  "name": "Ephect Dark (Modified)",
  "type": "dark",
  "colors": {
    "editor.background": "#1E1E1E",
    "editor.foreground": "#D4D4D4"
  },
  "tokenColors": [
    {
      "scope": ["keyword.control.ephect", "punctuation.definition.keyword.ephect"],
      "settings": {
        "foreground": "#ff938b",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["keyword.operator.word.ephect"],
      "settings": {
        "foreground": "#fe8173",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["variable.other.ephect", "punctuation.definition.variable.ephect"],
      "settings": {
        "foreground": "#7FDBCA"
      }
    },
    {
      "scope": [
        "meta.interpolation.ephect",
        "punctuation.definition.template-expression.begin.ephect",
        "punctuation.definition.template-expression.end.ephect"
      ],
      "settings": {
        "foreground": "#C678DD",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string.quoted.double", "string.quoted.single"],
      "settings": {
        "foreground": "#CE9178"
      }
    },
    {
      "scope": [
        "entity.name.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html"
      ],
      "settings": {
        "foreground": "#569CD6"
      }
    },
    {
      "scope": ["entity.other.attribute-name.html"],
      "settings": {
        "foreground": "#9CDCFE"
      }
    },
    {
      "scope": ["comment", "comment.block.html", "comment.line.double-slash"],
      "settings": {
        "foreground": "#6A9955",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["keyword.operator.heredoc.php"],
      "settings": {
        "foreground": "#FFB86C",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["variable.other.php"],
      "settings": {
        "foreground": "#D19A66",
        "fontStyle": ""
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#C586C0"
      }
    },
    {
      "scope": ["support.function", "entity.name.function"],
      "settings": {
        "foreground": "#61AFEF"
      }
    }
  ]
}
